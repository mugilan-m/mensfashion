<!-- CSS only -->
<title>Products-Mens Fashion</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
 
<div class="row row-cols-1 row-cols-md-3 g-4" >
  <%  detail.forEach(function(data){ %>
    <div class="card" style="width: 18rem;">
      <h5 class="card-title"><%= data.name %></h5>
      <img src="<%= data.image %>" class="card-img-top"  alt="" srcset="">
      <div class="card-body">
        <h5 class="card-title">PRICE</h5>
        <p class="card-text"><p><span style="color: green;"><%= data.price %></span></p><span style="color: red;"><del>400</del></span></p>
        <p class="card-text"><h1><span style="color: black;"><%= data.discription %></span></h1></p>
        <a href="#" class="btn btn-success">Update</a>
        <a href="#" class="btn btn-danger">Remove</a>
        
      </div>
    </div>
</div>
  <%    }) %>
</div>
  
</body>
</html>
